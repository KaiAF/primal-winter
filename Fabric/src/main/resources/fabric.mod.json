{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "A horrible accident has left the world - the entire world - a frozen wasteland. Unending snowstorms rage across the land, from forests, to plains, to jungles, to deserts. The air is thick with fog and wind howls across the landscape. Will you be able to survive in this primordial winter?",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/primalwinter",
    "sources": "https://github.com/alcatrazEscapee/primal-winter"
  },
  "license": "MIT",
  "icon": "assets/primalwinter/logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.alcatrazescapee.primalwinter.FabricPrimalWinter"
    ],
    "client": [
      "com.alcatrazescapee.primalwinter.FabricPrimalWinterClient"
    ]
  },
  "mixins": [
    "primalwinter.mixins.json",
    "primalwinter.common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.13",
    "fabric": "*",
    "minecraft": "1.19.x",
    "java": ">=17"
  }
}
  